{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node16",
    "lib": [
      "dom",
      "dom.iterable",
      "scripthost",
      "es2020"
    ],
    "jsx": "react-jsx",
    "types": [
      "vite/client",
      "vitest/globals"
    ],
    "baseUrl": "./",
    "paths": {
      "assets/*": [
        "./src/assets/*"
      ],
      "client-comp": [
        "./src/components/client"
      ],
      "server-comp": [
        "./src/components/server"
      ],
      "fallback-comp": [
        "./src/components/fallback"
      ],
      "graphql-query": [
        "./src/graphql/index.query"
      ],
      "layouts/*": [
        "./src/layouts/index.layouts"
      ],
      "lib": [
        "./src/lib/index.lib"
      ],
      "routes/*": [
        "./src/routes/*"
      ],
      "styles/*": [
        "./src/styles/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "**/*.js",
    "**/*.jsx"
  ]
}